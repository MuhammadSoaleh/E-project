@model dm.Models.Employees
@{
    ViewData["Title"] = "Add Employee";

}

<h1>Add Employee</h1>

<form asp-action="AddEmployee" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="About"></label>
        <input asp-for="About" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Designation"></label>
        <input asp-for="Designation" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Image"></label>
        <input type="file" name="imgfile" class="form-control-file" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
